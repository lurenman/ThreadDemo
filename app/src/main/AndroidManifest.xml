<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lurenman.threaddemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ScheduledThreadPoolLActivity" />
        <activity android:name=".activity.CallableTestActivity" />
        <activity android:name=".activity.AsyncTaskActivity" />
        <activity android:name=".activity.ThreadActivity" />
        <activity android:name=".activity.ThreadPoolExecutorActivity" />
        <activity android:name=".MainActivity1" />
        <activity android:name=".activity.ReentrantLockActivity" />
        <activity android:name=".activity.InterruptActivity" />
        <activity android:name=".activity.FutureTaskActivity" />
        <activity android:name=".activity.HandlerActivity" />
        <activity android:name=".activity.HandlerThreadActivity" />
        <activity android:name=".activity.IntentServiceActivity" />
        <activity android:name=".activity.AidlActivity" />

        <service android:name=".service.MyIntentService" />
        <!--android:process=":remote"-->
        <service
            android:name=".service.AidlTestService"
            android:exported="true">
            <intent-filter>
                <action android:name="com.lrm.aidl" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </service>
    </application>

</manifest>